  },
  "homepage": "https://github.com/heldrida/reactatouille-boilerplate#readme",
  "dependencies": {
    "babel-polyfill": "^6.22.0",
    "bluebird": "^3.4.7",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
