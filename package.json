    "webpack2",
    "boilerplate",
    "ES6",
    "ES2015",
    "babel",
    "mocha",
    "hmr",
    "express",
    "sass",
    "gsap"
  ],
  "author": "Punkbit",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/heldrida/reactatouille-boilerplate/issues"
  },
